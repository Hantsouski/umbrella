{
	"name": "@thi.ng/sax",
	"version": "2.1.72",
	"description": "Transducer-based, SAX-like, non-validating, speedy & tiny XML parser",
	"type": "module",
	"module": "./index.js",
	"typings": "./index.d.ts",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/thi-ng/umbrella.git"
	},
	"homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/sax#readme",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/postspectacular"
		},
		{
			"type": "patreon",
			"url": "https://patreon.com/thing_umbrella"
		}
	],
	"author": "Karsten Schmidt (https://thi.ng)",
	"license": "Apache-2.0",
	"scripts": {
		"build": "yarn clean && tsc --declaration",
		"clean": "rimraf --glob '*.js' '*.d.ts' '*.map' doc internal",
		"doc": "typedoc --excludePrivate --excludeInternal --out doc src/index.ts",
		"doc:ae": "mkdir -p .ae/doc .ae/temp && api-extractor run --local --verbose",
		"doc:readme": "yarn doc:stats && tools:readme",
		"doc:stats": "tools:module-stats",
		"pub": "yarn npm publish --access public",
		"test": "testament test"
	},
	"dependencies": {
		"@thi.ng/api": "^8.9.6",
		"@thi.ng/strings": "^3.6.1",
		"@thi.ng/transducers": "^8.8.3",
		"@thi.ng/transducers-fsm": "^2.2.39"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.38.0",
		"@thi.ng/testament": "^0.3.24",
		"rimraf": "^5.0.5",
		"tools": "workspace:^",
		"typedoc": "^0.25.2",
		"typescript": "^5.2.2"
	},
	"keywords": [
		"async",
		"fsm",
		"parser",
		"transducer",
		"tree",
		"typescript",
		"xml"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=12.7"
	},
	"files": [
		"./*.js",
		"./*.d.ts"
	],
	"exports": {
		".": {
			"default": "./index.js"
		}
	},
	"thi.ng": {
		"related": [
			"hiccup-svg",
			"parse",
			"transducers",
			"transducers-fsm"
		],
		"year": 2018
	}
}
