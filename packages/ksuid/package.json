{
	"name": "@thi.ng/ksuid",
	"version": "3.2.15",
	"description": "Configurable K-sortable unique IDs, ULIDs, binary & base-N encoded, 32/48/64bit time resolutions",
	"type": "module",
	"module": "./index.js",
	"typings": "./index.d.ts",
	"bin": "bin/ksuid",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/thi-ng/umbrella.git"
	},
	"homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/ksuid#readme",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/postspectacular"
		},
		{
			"type": "patreon",
			"url": "https://patreon.com/thing_umbrella"
		}
	],
	"author": "Karsten Schmidt (https://thi.ng)",
	"license": "Apache-2.0",
	"scripts": {
		"bench": "tools:node-esm bench/index.ts",
		"build": "yarn clean && tsc --declaration",
		"clean": "rimraf --glob '*.js' '*.d.ts' '*.map' doc",
		"doc": "typedoc --excludePrivate --excludeInternal --out doc src/index.ts",
		"doc:ae": "mkdir -p .ae/doc .ae/temp && api-extractor run --local --verbose",
		"doc:readme": "bun ../../tools/src/module-stats.ts && bun ../../tools/src/readme.ts",
		"pub": "yarn npm publish --access public",
		"test": "bun test"
	},
	"dependencies": {
		"@thi.ng/base-n": "^2.5.15",
		"@thi.ng/errors": "^2.4.1",
		"@thi.ng/random": "^3.6.13",
		"@thi.ng/strings": "^3.6.5"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.38.2",
		"@thi.ng/testament": "^0.4.0",
		"rimraf": "^5.0.5",
		"tools": "workspace:^",
		"typedoc": "^0.25.3",
		"typescript": "^5.2.2"
	},
	"keywords": [
		"32bit",
		"64bit",
		"base62",
		"bigint",
		"binary",
		"bytes",
		"crypto",
		"database",
		"dynamodb",
		"encode",
		"epoch",
		"generator",
		"id",
		"millisecond",
		"random",
		"sort",
		"timestamp",
		"typescript",
		"ulid",
		"uuid"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=12.7"
	},
	"files": [
		"./*.js",
		"./*.d.ts",
		"bin"
	],
	"exports": {
		".": {
			"default": "./index.js"
		},
		"./aksuid": {
			"default": "./aksuid.js"
		},
		"./api": {
			"default": "./api.js"
		},
		"./cli": {
			"default": "./cli.js"
		},
		"./ksuid32": {
			"default": "./ksuid32.js"
		},
		"./ksuid64": {
			"default": "./ksuid64.js"
		},
		"./ulid": {
			"default": "./ulid.js"
		}
	},
	"thi.ng": {
		"related": [
			"base-n",
			"idgen",
			"random"
		],
		"status": "stable",
		"year": 2020
	}
}
