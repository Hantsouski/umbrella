	"scripts": {
		"start": "vite --host --open",
		"css:gen": "../../node_modules/.bin/metacss generate --pretty --out css/framework.json ../../packages/meta-css/specs",
		"css:watch": "../../node_modules/.bin/metacss convert --watch --pretty --out css/style.css --specs css/framework.json --force @css/includes.txt -I css/anim.css css/style.meta",
		"css:build": "../../node_modules/.bin/metacss convert --out css/style.css --specs css/framework.json --force @css/includes.txt -I css/anim.css css/style.meta",
		"build": "tsc && yarn css:build && vite build --base='./'",
		"preview": "vite preview --host --open"
	},
	"devDependencies": {
		"typescript": "^5.3.2",
		"vite": "^5.0.4"
	},
	"dependencies": {
