{
	"name": "@thi.ng/rstream-graph",
	"version": "4.1.88",
	"description": "Declarative dataflow graph construction for @thi.ng/rstream",
	"type": "module",
	"module": "./index.js",
	"typings": "./index.d.ts",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/thi-ng/umbrella.git"
	},
	"homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/rstream-graph#readme",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/postspectacular"
		},
		{
			"type": "patreon",
			"url": "https://patreon.com/thing_umbrella"
		}
	],
	"author": "Karsten Schmidt (https://thi.ng)",
	"license": "Apache-2.0",
	"scripts": {
		"build": "yarn clean && tsc --declaration",
		"clean": "rimraf --glob '*.js' '*.d.ts' '*.map' doc nodes",
		"doc": "typedoc --excludePrivate --excludeInternal --out doc src/index.ts",
		"doc:ae": "mkdir -p .ae/doc .ae/temp && api-extractor run --local --verbose",
		"doc:readme": "bun ../../tools/src/module-stats.ts && bun ../../tools/src/readme.ts",
		"pub": "yarn npm publish --access public",
		"test": "bun test"
	},
	"dependencies": {
		"@thi.ng/api": "^8.9.9",
		"@thi.ng/atom": "^5.2.15",
		"@thi.ng/checks": "^3.4.9",
		"@thi.ng/errors": "^2.4.3",
		"@thi.ng/paths": "^5.1.50",
		"@thi.ng/resolve-map": "^7.1.43",
		"@thi.ng/rstream": "^8.2.11",
		"@thi.ng/transducers": "^8.8.12"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.38.3",
		"@thi.ng/testament": "^0.4.2",
		"rimraf": "^5.0.5",
		"tools": "workspace:^",
		"typedoc": "^0.25.4",
		"typescript": "^5.3.2"
	},
	"keywords": [
		"animation",
		"async",
		"compute",
		"dataflow",
		"datastructure",
		"graph",
		"reactive",
		"rstream",
		"stream",
		"topology",
		"typescript"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=12.7"
	},
	"files": [
		"./*.js",
		"./*.d.ts",
		"nodes"
	],
	"exports": {
		".": {
			"default": "./index.js"
		},
		"./api": {
			"default": "./api.js"
		},
		"./graph": {
			"default": "./graph.js"
		},
		"./nodes/extract": {
			"default": "./nodes/extract.js"
		},
		"./nodes/math": {
			"default": "./nodes/math.js"
		}
	},
	"thi.ng": {
		"parent": "@thi.ng/rstream",
		"related": [
			"dot",
			"resolve-map",
			"rstream-dot"
		],
		"year": 2018
	}
}
